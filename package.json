{
  "name": "line-monitoring-service-admin",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev:server": "tsx watch server/index.ts",
    "dev:client": "vite",
    "dev": "concurrently -k -n SERVER,CLIENT \"npm:dev:server\" \"npm:dev:client\"",
    "build:client": "tsc -b && vite build",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "typecheck": "tsc -b --pretty",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write ."
  },
  "dependencies": {
    "@mantine/core": "^8.2.4",
    "@mantine/hooks": "^8.2.4",
    "@mantine/notifications": "^8.2.4",
    "@tabler/icons-react": "^3.34.1",
    "@tanstack/react-query": "^5.51.15",
    "modern-css-reset": "^1.4.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^7.8.0",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/express": "^5.0.3",
    "@types/node": "^24.2.1",
    "@types/react": "^18.3.8",
    "@types/react-dom": "^18.3.0",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^9.2.0",
    "cors": "^2.8.5",
    "eslint": "^8.57.0",
